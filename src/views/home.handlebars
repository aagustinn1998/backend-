<div class="container">
    <h2>Lista de Productos</h2>
    <table>
        <thead>
            <tr>
                <th>Título</th>
                <th>Descripción</th>
                <th>Precio</th>
                <th>Thumbnail</th>
                <th>Código</th>
                <th>Stock</th>
            </tr>
        </thead>
        <tbody id="listProducts">
            {{#each productos}}
            <tr>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td><img src="{{this.thumbnailUrl}}"></td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

